{
  "id": "claude-3.7-sonnet",
  "displayName": "Claude 3.7 Sonnet",
  "provider": "anthropic",
  "apiModel": "claude-3-7-sonnet-latest",
  "enabled": true,
  "routing": {
    "service": "anthropic",
    "endpoint": "https://api.anthropic.com/v1/messages"
  },
  "globalRateLimit": {
    "requests": 1000,
    "window": {
      "amount": 1,
      "unit": "hour"
    }
  },
  "userRateLimit": {
    "requests": 50,
    "window": {
      "amount": 6,
      "unit": "hours"
    }
  },
  "parameters": {
    "temperature": 0.7,
    "max_tokens": 64000,
    "top_p": 1.0,
    "top_k": 40
  },
  "capabilities": {
    "streaming": true,
    "vision": true,
    "functionCalling": true,
    "webSearch": true
  }
}