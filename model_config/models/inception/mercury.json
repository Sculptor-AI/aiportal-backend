{
  "id": "mercury",
  "displayName": "Mercury",
  "provider": "inception",
  "apiModel": "inception/mercury",
  "enabled": true,
  "routing": {
    "service": "openrouter",
    "endpoint": "https://openrouter.ai/api/v1/chat/completions"
  },
  "globalRateLimit": {
    "requests": 2000,
    "window": {
      "amount": 1,
      "unit": "hour"
    }
  },
  "userRateLimit": {
    "requests": 100,
    "window": {
      "amount": 6,
      "unit": "hours"
    }
  },
  "parameters": {
    "temperature": 0.7,
    "max_tokens": 16000,
    "top_p": 1.0,
    "frequency_penalty": 0,
    "presence_penalty": 0
  },
  "capabilities": {
    "streaming": true,
    "vision": false,
    "functionCalling": false,
    "webSearch": true,
    "speed": "5-10x faster than traditional models"
  },
  "description": "Mercury is the first diffusion large language model (dLLM). Applying a breakthrough discrete diffusion approach, the model runs 5-10x faster than even speed optimized models."
} 